#include "globalVar.h"

void sendall(){
	// Base_System_status
	registerFrame[0x01] = 0;
	// Vacuum Status
	registerFrame[0x02] = 0;
	// Movement Status
	registerFrame[0x03] = 0;
	// Movement Actual Status
	registerFrame[0x04] = 0;
	// z-axis Moving Status
	registerFrame[0x10] = 0;
	// z-axis Actual Position
	registerFrame[0x11] = 0;
	// z-axis Actual Speed
	registerFrame[0x12] = 0;
	// z-axis Actual Acceleration
	registerFrame[0x13] = 0;
	// Pick order
	registerFrame[0x21] = 0;
	// Place order
	registerFrame[0x22] = 0;
	// 1st shelves position
	registerFrame[0x23] = 0;
	// 2nd shelves position
	registerFrame[0x24] = 0;
	// 3rd shelves position
	registerFrame[0x25] = 0;
	// 4th shelves position
	registerFrame[0x26] = 0;
	// 5th shelves position
	registerFrame[0x27] = 0;
	// Goal point z
	registerFrame[0x30] = 0;
	//x-axis Actual position
	registerFrame[0x40] = 0;


}
